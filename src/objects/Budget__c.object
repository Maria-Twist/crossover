        <externalId>false</externalId>
        <formula>IF (
    Expected_Revenue__c = 0 ||
    ISBLANK(Expected_Revenue__c) ||
    Revenue_Target__c = 0 ||
    ISBLANK(Revenue_Target__c),
    0,
    Expected_Revenue__c / Revenue_Target__c)</formula>
